<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company />
    <Authors>xiaozg</Authors>
    <Description>very easy to use httpclient for webapi. call remote methods like local methods. support async methods too.
让远程方法（api接口）像本地方法一样方便调用，支持异步。</Description>
    <PackageTags>webapi,easyinvoke,httpclientfactory</PackageTags>
    <PackageProjectUrl>https://github.com/geeking/WebApiEasyInvoker</PackageProjectUrl>
    <Copyright>MIT License</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DispatchProxyAsync\DispatchProxyAsync.csproj" />
  </ItemGroup>

</Project>
